<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stussy.stussyclone20220929junil.repository.AccountRepository">
    <insert id="save" parameterType="com.stussy.stussyclone20220929junil.domain.User">
        insert into
            user_mst
        values(
            0,
            #{username},
            #{oauth_username},
            #{password},
            #{name},
            #{email},
            #{provider},
            1,
            now(),
            now()
        )
    </insert>
</mapper>